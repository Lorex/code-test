<?xml version="1.0" encoding="UTF-8"?><web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>KSACWeb</display-name>
    <servlet>
        <description/>
        <display-name>Home</display-name>
        <servlet-name>Home</servlet-name>
        <servlet-class>Home</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>Login</display-name>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>AdminPanel</display-name>
        <servlet-name>AdminPanel</servlet-name>
        <servlet-class>AdminPanel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tiles2Servet</servlet-name>
        <servlet-class>org.apache.tiles.web.startup.TilesServlet</servlet-class>
        <init-param>
            <param-name>org.apache.tiles.impl.BasicTilesContainer.DEFINITIONS_CONFIG</param-name>
            <param-value>/WEB-INF/tiles20-defs.xml</param-value>
        </init-param>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <description/>
        <display-name>Logout</display-name>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>CreatePost</display-name>
        <servlet-name>CreatePost</servlet-name>
        <servlet-class>CreatePost</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>EditPost</display-name>
        <servlet-name>EditPost</servlet-name>
        <servlet-class>EditPost</servlet-class>
    </servlet>
    <servlet>
        <display-name>IPRecord</display-name>
        <servlet-name>IPRecord</servlet-name>
        <servlet-class>IPRecord</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>IPCounter</display-name>
        <servlet-name>IPCounter</servlet-name>
        <servlet-class>IPCounter</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>News</display-name>
        <servlet-name>News</servlet-name>
        <servlet-class>News</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminPanel</servlet-name>
        <url-pattern>/AdminPanel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreatePost</servlet-name>
        <url-pattern>/CreatePost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditPost</servlet-name>
        <url-pattern>/EditPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IPRecord</servlet-name>
        <url-pattern>/IPRecord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IPCounter</servlet-name>
        <url-pattern>/IPCounter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>News</servlet-name>
        <url-pattern>/News</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <page-encoding>UTF-8</page-encoding>
        </jsp-property-group>
    </jsp-config>
</web-app>